fn fizzBuzz(i) {
    if (i % 15 == 0) { return "FizzBuzz" }
    elseif (i % 3 == 0) { return "Fizz" }
    elseif (i % 5 == 0) { return "Buzz" }
    else { return i }
}

v = []
count = 0
start = getTime()
while count < 1000 {
    v.push(fizzBuzz(count))
    count = count + 1
}
end = getTime()
print(v)
print(getElapsedTimeMS(start, end))

